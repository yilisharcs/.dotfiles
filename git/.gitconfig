[user]
	name = yilisharcs
	email = yilisharcs@gmail.com
	signingkey = 1D1CA99103BF8677
[init]
	defaultBranch = main
[core]
	editor = nvim
	excludesfile = ~/.config/git/ignore
[diff]
	tool = nvimdiff
[merge]
	tool = nvimdiff
[pull]
	rebase = true
[rebase]
	autoStash = true
[alias]
	last = log -1 HEAD
	logd = log --decorate --oneline --graph
	unstage = reset HEAD --
[commit]
	gpgSign = true
[tag]
	gpgSign = true
